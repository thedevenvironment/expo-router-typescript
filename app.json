{
  "expo": {
    "name": "Expo Router Typescript",
    "slug": "expo-router-typescript",
    "scheme": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/app-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash.png",
      "backgroundColor": "#222"
    },
    "experiments": {
      "reactServerFunctions": false
    },
    "assetBundlePatterns": ["assets/*"],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app-icon.png",
        "backgroundColor": "#222"
      },
      "package": "com.haytherecharlie.exporoutertypescript"
    },
    "web": {
      "favicon": "./src/assets/images/app-icon.png",
      "output": "static",
      "bundler": "metro"
    },
    "plugins": ["expo-router", "expo-font"],
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
